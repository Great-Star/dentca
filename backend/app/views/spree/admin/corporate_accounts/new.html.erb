<% content_for :page_title do %>
    <%= link_to Spree.t(:corporate_accounts), spree.admin_corporate_accounts_url %> /
    <%= Spree.t(:new_corporate_account) %>
<% end %>

<div data-hook="admin_corporate_account_new_form_header">
    <%= render partial: "spree/admin/shared/error_messages", locals: { target: @corporate_account } %>
</div>

<div data-hook="admin_new_corporate_account_new_form">
    <%= form_for [:admin, @corporate_account], url: spree.admin_corporate_accounts_url, method: :post do |f| %>
        <%= render partial: "form", locals: {f: f} %>

        <div data-hook="admin_corporate_account_new_form_buttons">
            <%= render partial: "spree/admin/shared/new_resource_links" %>
        </div>
    <% end %>
</div>